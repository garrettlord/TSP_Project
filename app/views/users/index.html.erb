<% provide(:title, 'Users') %>
<h1>Listing Users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Phone Number</th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.phone_number %></td>
    <td><%= link_to 'Show', user_path(@user) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>