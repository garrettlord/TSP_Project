<% provide(:title, 'New Group') %>
<h1>New Group</h1>
<p>Find me in app/views/groups/new.html.erb</p>

<%= form_for(@group) do |f| %>
  <%= render 'error_messages' %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.submit "Create new group", class: "btn btn-large btn-primary" %>
<% end %>